<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Data Driven Test Case">

  <listeners>
    <listener class-name="org.uncommons.reportng.HTMLReporter"/>
    <listener class-name="org.uncommons.reportng.JUnitXMLReporter"/>
    <listener class-name="com.resources.base.Listeners"/>
  </listeners>

  <test name="Run with cucumber">
    <classes>
      <class name="com.resources.cucumberclass.TestRunner"/>
    </classes>
  </test>
  
</suite>
